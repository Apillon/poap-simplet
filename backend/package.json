{
  "name": "ps-poap-api",
  "version": "0.0.1",
  "description": "Prebuild solution for Proof of attendance via Apillon API.",
  "main": "index.js",
  "scripts": {
    "test": "npx jest --config jestconfig.json --forceExit --detectOpenHandles --runInBand",
    "development:run": "nodemon -r ts-node/register --inspect ./src/scripts/start-http",
    "cron": "node -r ts-node/register --inspect ./src/scripts/start-cron",
    "dev": "npm run development:run",
    "tsc": "tsc",
    "build": "tsc",
    "db-upgrade": "node -r ts-node/register ./src/scripts/db/upgrade-db",
    "db-upgrade:ci": "node -r ts-node/register ./src/scripts/db/upgrade-db --F",
    "db-downgrade": "node -r ts-node/register ./src/scripts/db/downgrade-db",
    "db-rebuild": "node -r ts-node/register ./src/scripts/db/rebuild-db",
    "db-drop": "node -r ts-node/register ./src/scripts/db/drop-db",
    "format": "prettier --write \"**/*.{ts,tsx,md}\""
  },
  "dependencies": {
    "@apillon/sdk": "3.8.3",
    "@rawmodel/core": "^3.3.4",
    "@rawmodel/parsers": "^3.3.4",
    "@rawmodel/validators": "^3.3.4",
    "body-parser": "1.20.3",
    "colors": "^1.4.0",
    "cors": "^2.8.5",
    "cron": "^3.5.0",
    "dotenv": "^16.4.7",
    "express": "^5.0.1",
    "handlebars": "^4.7.8",
    "jsonwebtoken": "^9.0.2",
    "moment": "^2.30.1",
    "mysql2": "^3.12.0",
    "nodemailer": "^6.10.0",
    "reflect-metadata": "^0.2.2",
    "short-unique-id": "^5.2.0",
    "sqlstring": "^2.3.3",
    "ts-node": "^10.9.2"
  },
  "devDependencies": {
    "@types/cron": "^2.4.3",
    "@types/express": "^4.17.21",
    "@types/handlebars": "^4.1.0",
    "@types/jest": "^29.5.14",
    "@types/supertest": "^2.0.16",
    "ethers": "^6.13.5",
    "jest": "^29.7.0",
    "nodemon": "^3.1.9",
    "supertest": "^6.3.3",
    "ts-jest": "^29.2.5",
    "ts-mysql-migrate": "^1.1.2",
    "typescript": "^5.7.3"
  },
  "author": "Apillon",
  "license": "ISC"
}
